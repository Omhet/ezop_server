msgid_plural "@count users accessing site; throttle enabled."
msgstr[0] "@count пользователь на сайте; регулятор включен."
msgstr[1] "@count пользователя на сайте; регулятор включен."
msgstr[2] "@count пользователей на сайте; регулятор включен."

#: modules/throttle/throttle.module:101
